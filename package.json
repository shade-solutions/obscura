{
  "name": "obscura-icons",
  "version": "1.1.0",
  "description": "A minimal React icon library with unique, rare, and niche icons that other libraries miss",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "type": "module",
  "scripts": {
    "build": "npm run clean && npm run generate-icons && npm run build-lib",
    "clean": "rm -rf dist",
    "generate-icons": "node scripts/generate-icons.js",
    "build-lib": "rollup -c",
    "dev": "rollup -c -w",
    "prepublishOnly": "npm run build",
    "test": "echo \"No tests yet\" && exit 0"
  },
  "keywords": [
    "react",
    "icons",
    "svg",
    "ai",
    "saas",
    "developer",
    "ui",
    "components",
    "rare-icons",
    "niche-icons"
  ],
  "author": "Shaswat Raj <shaswat@shade.so>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/shade-solutions/obscura.git"
  },
  "homepage": "https://shade-solutions.github.io/obscura",
  "bugs": {
    "url": "https://github.com/shade-solutions/obscura/issues"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/react": "^18.2.45",
    "react": "^18.2.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.9.1",
    "rollup-plugin-dts": "^6.1.0",
    "svgo": "^3.2.0",
    "tslib": "^2.8.1",
    "typescript": "^5.3.3"
  }
}
